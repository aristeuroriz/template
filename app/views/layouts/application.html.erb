				<div class="collapse navbar-collapse" id="bar">

					<div class="container">
						<%= render "layouts/navigation" %>
					</div>

				</div><!-- /.navbar-collapse -->
			</nav>
		</header>
		

		<div id="main" role="main">

			<div class="container">
				<div class="content principal">
					<div class="row-fluid">
					

						<%= bootstrap_flash %> 

